{
  "name": "@virgo/api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "nest start",
    "predev": "pnpm --filter @virgo/config run build && node ../../tools/ensure-api-dist-entry.cjs",
    "dev": "nest start --watch",
    "prestart:dev": "pnpm run predev",
    "start:dev": "pnpm run dev",
    "build": "nest build",
    "lint": "eslint \"src/**/*.ts\"",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "test": "pnpm test:e2e",
    "test:e2e": "jest --config jest-e2e.json"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.738.0",
    "@aws-sdk/s3-request-presigner": "3.738.0",
    "@nestjs/common": "11.0.5",
    "@nestjs/config": "4.0.0",
    "@nestjs/core": "11.0.5",
    "@nestjs/mapped-types": "2.1.0",
    "@nestjs/platform-express": "11.0.5",
    "@prisma/client": "5.18.0",
    "@virgo/config": "workspace:*",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "prom-client": "^15.1.3",
    "reflect-metadata": "0.2.1",
    "rxjs": "7.8.1"
  },
  "devDependencies": {
    "@eslint/js": "9.9.0",
    "@nestjs/cli": "11.0.5",
    "@nestjs/schematics": "11.0.0",
    "@nestjs/testing": "11.0.5",
    "@types/express": "^5.0.5",
    "@types/jest": "29.5.12",
    "@types/node": "20.11.30",
    "@types/supertest": "2.0.16",
    "eslint": "9.9.0",
    "jest": "29.7.0",
    "prisma": "5.18.0",
    "supertest": "7.0.0",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.2",
    "typescript": "5.7.3",
    "typescript-eslint": "8.47.0"
  }
}
